@-moz-document url-prefix() {
#page-content-wrapper{
height:538px;
}
}
.file-modal-info {
padding: 37px;
margin-top: -40px;
}
.show-sidebar{
position:absolute;
left:10px;
bottom:10px;
}
body{
overflow-y:hidden; overflow-x:hidden;
}
iframe{border:0;}
.sidebar-group ul li{
display:inline; 
}
.sidebar-group ul{
margin-top:-10px;
}

@media(max-width:990px){
.navey h1{
top:-10px;
font-size:40px;
}
#siteseal{
display:none;
}
.trademark{ 
display:none;
}
}
.navey h1{
position:absolute;
top:-16px;
text-shadow:2px 2px #000;
opacity:0.9;
color:whitesmoke;
left:-162px;
font-size:50px;
}
#files p {
margin-top:-56px;
color:white;
}
.navbar-inverse .navbar-nav > li > a {
color:white;
}
.bottom-navy{
background-color:#47a447;
}
.thumbnail > img, .thumbnail a > img {
min-height:123px;
max-height:123px;
}
.thumbnail-download{
position:absolute;
bottom:29px;
right:25px;
}
.c2c{
color:whitesmoke; margin-top:-33px; text-align:right; margin-right:4px;
}
#wrapper {
  padding-left: 250px;
  transition: all 0.4s ease 0s;
}

#sidebar-wrapper {
border-right:1px solid white;
  margin-left: -250px;
  left: 250px;
  width: 250px;
  background-color:#47a447;
  position: fixed;
  height: 100%;
  overflow-y: auto;
  z-index: 1000;
  transition: all 0.4s ease 0s;
}

#page-content-wrapper {
  width: 100%;
  min-height: 89%;
  height: 89%;
}

.sidebar-nav {
  position: absolute;
  top: 0;
  width: 250px;
  list-style: none;
  margin: 0;
  padding: 0;
  padding-top:35px;
}

.sidebar-nav li {
  line-height: 40px;
  text-indent: 20px;
}

.sidebar-nav li a {
  color: white;
  display: block;
  text-decoration: none;
}

.sidebar-nav li a:hover {
  color: #fff;
  background: rgba(255,255,255,0.2);
  text-decoration: none;
}

.sidebar-nav li a:active,
.sidebar-nav li a:focus {
  text-decoration: none;
}

.sidebar-nav > .sidebar-brand {
  height: 65px;
  line-height: 60px;
  font-size: 18px;
}

.sidebar-nav > .sidebar-brand a {
  color: #999999;
}

.sidebar-nav > .sidebar-brand a:hover {
  color: #fff;
  background: none;
}

.content-header {
  height: 65px;
  line-height: 65px;
}

.content-header h1 {
  margin: 0;
  margin-left: 20px;
  line-height: 65px;
  display: inline-block;
}
.inset {
  padding: 20px;
}
@media (max-width:1100px){
min-height:89.9%;
}
#page-content-wrapper {
}


@media (max-width:767px) {
.bottom-navy{
display:none;
}
#page-content-wrapper{
min-height:100%;
}
#wrapper {
  padding-left: 0;
}

#sidebar-wrapper {
  left: 0;
}

#wrapper.active {
  position: relative;
  left: 250px;
}
#wrapper.active #sidebar-wrapper {
  left: 250px;
  width: 250px;
  transition: all 0.4s ease 0s;
}
#menu-toggle {
  display: inline-block;
}

.inset {
  padding: 15px;
}

}

